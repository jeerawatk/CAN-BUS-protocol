  u8g.drawBitmapP(0, 0, 16, 64, logo);
  //u8g.setFont(u8g_font_04b_03r);
  //u8g.drawStr( 15, 30, "www.ioinnovationlab.com");
  //u8g.drawStr( 15, 40, "version 1.0");  
}

void engineRPM(void){
  tCAN message;
if (mcp2515_check_message()) 
  {
    if (mcp2515_get_message(&message)) 
  {
        if(message.id == 0x158 )  //uncomment when you want to filter
             {
              //speeds = ((((message.data[4]*256)+message.data[5])*0.62137119)/100)*1.609344;
              speedA=message.data[4];
              speedB=message.data[5];
              speeds = ((((speedA*256)+speedB)*0.62137119)/100)*1.609344;
              RPM = (message.data[2]*256)+message.data[3];
     
  }
       // else if(message.id == 0x1DC )  //uncomment when you want to filter
       //      {
       //       RPM = ((message.data[1]*256)+message.data[2])+message.data[3];
  //}
         else if(message.id == 0x188){
             if(speeds >= 20 && message.data[3] == 0x08 && lockdoor == 1){
                lockdoor=0; // lock
             }
             else if(message.data[3] == 0x01 && lockdoor == 0){
                lockdoor=0;
             }
         }
           }}
}


