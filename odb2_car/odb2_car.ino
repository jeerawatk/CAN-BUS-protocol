void setup()
{
  u8g.firstPage();
Serial.begin(9600);
Serial.println("CAN-Bus Demo");

if(Canbus.init(CANSPEED_500))  /* Initialise MCP2515 CAN controller at the specified speed */
  {
    Serial.println("CAN Init ok");
  } else
  {
    Serial.println("Can't init CAN");
  } 
  do
  {
    intro();
  }
  while (u8g.nextPage());
  delay(3000);
}
 
 
void loop(void) 
{
  buttonState = digitalRead(buttonPin);

  // picture loop
  //if (buttonState == HIGH) {
  //  mode=mode+1;
  //  delay(100);
 // }
  
  u8g.firstPage();  
  do {
    if(mode==1){
      engineRPM();
      mode1();
    }
    else if(mode==2){
      mode2();
    }
    else if(mode==3){
      mode3();
    }
    else if(mode==4){
      mode = 1;
    }
  } while( u8g.nextPage() );
}
 
